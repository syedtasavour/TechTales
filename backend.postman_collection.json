		{
			"name": "admin",
			"item": [
				{
					"name": "toggleBlogApprovalStatus",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var template = `\r",
									"<canvas id=\"lineChart\" height=\"200\"></canvas>\r",
									"\r",
									"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.js\"></script> \r",
									"\r",
									"<script>\r",
									"    var ctx = document.getElementById(\"lineChart\");\r",
									"\r",
									"    var lineChart = new Chart(ctx, {\r",
									"        type: \"line\",\r",
									"        data: {\r",
									"            labels: [],\r",
									"            datasets: [{\r",
									"                data: [],\r",
									"                label: 'Views',\r",
									"                borderColor: \"#3e95cd\",\r",
									"                fill: false\r",
									"            }]\r",
									"        },\r",
									"        options: {\r",
									"            legend: { display: false },\r",
									"            title: {\r",
									"                display: true,\r",
									"                text: 'Blog Views Over Time'\r",
									"            },\r",
									"            scales: {\r",
									"                xAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: 'Date'\r",
									"                    }\r",
									"                }],\r",
									"                yAxes: [{\r",
									"                    display: true,\r",
									"                    scaleLabel: {\r",
									"                        display: true,\r",
									"                        labelString: 'Views'\r",
									"                    }\r",
									"                }]\r",
									"            }\r",
									"        }\r",
									"    });\r",
									"\r",
									"    pm.getData(function (err, value) {\r",
									"        lineChart.data.datasets[0].data = [value.response.data.views];\r",
									"        lineChart.data.labels = [value.response.data.createdAt];\r",
									"        lineChart.update();\r",
									"    });\r",
									"\r",
									"</script>`;\r",
									"\r",
									"function constructVisualizerPayload() {\r",
									"    var res = pm.response.json();\r",
									"    \r",
									"    var visualizerData = {\r",
									"        // Labels take an array of strings\r",
									"        labels: [res.data.createdAt],\r",
									"        \r",
									"        // Data takes an array of numbers\r",
									"        data: [res.data.views]\r",
									"    };\r",
									"\r",
									"    return {response: visualizerData};\r",
									"}\r",
									"\r",
									"pm.visualizer.set(template, constructVisualizerPayload());"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"permalink\":  \"blog-permalink-here\",\r\n    \"status\":  \"approved\"\r\n\r\n}",
							"options": {
								"raw": {
									"language": "json"
